{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5067",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true, //default false, sirve para abrir el navegador 
      "launchUrl": "swagger", //visualiza los end-ponts de la API
      "applicationUrl": "https://localhost:7207;http://localhost:5067",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Saber en que tipo de entorno se envuentra, en Development o Production
      }
    }
  }
}
